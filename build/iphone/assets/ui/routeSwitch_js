var routeSwitch=function(color){var self=Ti.UI.createButton({textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER,title:color,color:"black",backgroundColor:"gray",backgroundImage:"none",backgroundDisabledColor:"white",backgroundDisabledImage:"none",width:'27%',height:"28%",left:"2%",top:"2%",right:"2%",bottom:"2%"});return self;};module.exports=routeSwitch;